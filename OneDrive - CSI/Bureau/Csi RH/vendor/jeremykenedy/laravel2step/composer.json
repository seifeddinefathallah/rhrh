{
    "name": "jeremykenedy/laravel2step",
    "description": "Laravel 2 Step Authentication Package",
    "keywords": [
        "laravel",
        "2-step verification",
        "verification"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "jeremykenedy",
            "email": "jeremykenedy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1|^8.2",
        "laravel/framework": "6.*|7.*|8.*|9.*|10.*"
    },
    "require-dev": {
        "illuminate/support": "^8.5",
        "laravel/laravel": "^8.0",
        "laravel/tinker": "^2.4",
        "orchestra/testbench": "^6.0",
        "suin/phpcs-psr4-sniff": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "jeremykenedy\\laravel2step\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jeremykenedy\\laravel2step\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jeremykenedy\\laravel2step\\Laravel2stepServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
